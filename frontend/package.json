{
  "name": "vue3-frontend",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "mobile:build": "webpack --mode production",
    "cap:sync": "npm run build && cap sync",
    "dev": "webpack serve --mode development --host 0.0.0.0 --port 5173",
    "preview": "vite preview",
    "cap:init": "cap init",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "cap:run:android": "npm run build && cap sync && cap run android",
    "cap:run:ios": "npm run build && cap sync && cap run ios",
    "mobile:dev": "vite --mode mobile",
    "cap:android:dev": "npm run mobile:build && cap sync android && cap open android",
    "cap:ios:dev": "npm run mobile:build && cap sync ios && cap open ios",
    "cap:android:build": "npm run mobile:build && cap sync android && cd android && ./gradlew assembleDebug",
    "cap:android:run": "npm run mobile:build && cap sync android && cap run android"
  },
  "dependencies": {
    "apexcharts": "^5.3.5",
    "axios": "^1.11.0",
    "pinia": "^2.1.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@vue/compiler-sfc": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.0",
    "postcss-loader": "^7.3.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vue-loader": "^17.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.2"
  }
}
